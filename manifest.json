{
  "manifest_version": 2,
  "name": "refined stack overflow",
  "description": "show upvoted answers / your answers first",
  "version": "0",
  "content_scripts": [
    {
      "matches": ["*://*.stackoverflow.com/*"],
      "js": ["script.js"],
      "css": ["style.css"]
    }
  ],
  "icons": {
    "48": "icon.png",
    "96": "icon@2x.png"
  }
}
